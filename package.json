{
  "name": "modelify.agency-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "socket.io-client": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:eslint": "eslint --fix src/**/*.{js,jsx}",
    "lint:stylelint": "stylelint --fix src/**/*.{scss,css}",
    "lint:prettier": "prettier --write src/**/*.{js,jsx,json,scss,css}",
    "lint": "npm run lint:prettier && npm run lint:stylelint && npm run lint:eslint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/src/setupTests.js",
      "!<rootDir>/src/index.jsx",
      "!<rootDir>/src/api.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.3",
    "history": "^4.9.0",
    "jest-localstorage-mock": "^2.4.0",
    "prettier": "^1.15.3",
    "react-test-context-provider": "^2.2.0",
    "react-test-renderer": "^16.8.4",
    "stylelint": "^9.10.0",
    "stylelint-config-recommended": "^2.1.0"
  }
}
